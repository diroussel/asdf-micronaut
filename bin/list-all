#!/bin/bash
curl -s https://github.com/micronaut-projects/micronaut-core/releases | grep -e ".*releases/.*micronaut.*.zip\"" | sed -e "s#^.*/v.*/##" -e "s#.zip.*##" -e "s#.*-##" | sort -t. -n | paste -s -d" " -
#curl -s https://github.com/micronaut-projects/micronaut-core/releases | grep -e ".*releases/.*micronaut.*.zip\"" | sed -e "s#^.*/v.*/##" -e "s#.zip.*##" -e "s#.*-##" | sort -t. -n | paste -s -d" " -